<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading"><h3>Students</h3></div>

    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>Nickname</th>
          <th>Email</th>
        </tr>
      </thead>

      <tbody>
        <% @students.each do |student| %>
          <tr>
            <td><%= student.name %></td>
            <td><%= student.nickname %></td>
            <td><%= student.email %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
